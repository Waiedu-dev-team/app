# Environment Variables Example
# Sao chép file này thành .env và cập nhật giá trị

# App Configuration
VITE_APP_NAME="WAI Education"
VITE_APP_URL="http://localhost:3000"
VITE_APP_ENV="development"

# API Configuration
VITE_API_URL="https://api.waieducation.com"
VITE_API_VERSION="v1"

# Third-party Services
VITE_GOOGLE_ANALYTICS_ID=""
VITE_FACEBOOK_PIXEL_ID=""

# Features Flags
VITE_ENABLE_ANALYTICS="false"
VITE_ENABLE_DEBUG="true"
VITE_ENABLE_PWA="false"

# Contact Information
VITE_CONTACT_EMAIL="contact@waieducation.com"
VITE_SUPPORT_EMAIL="support@waieducation.com"

# Social Media
VITE_FACEBOOK_URL=""
VITE_TWITTER_URL=""
VITE_LINKEDIN_URL=""
VITE_YOUTUBE_URL="" 